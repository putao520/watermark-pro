{
  "private": true,
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^6.5.0",
    "@pansy/react-watermark": "^2.1.1",
    "@types/canvas-confetti": "^1.4.0",
    "@types/file-saver": "^2.0.2",
    "@types/html2canvas": "^0.0.36",
    "@umijs/preset-react": "1.x",
    "ahooks": "^2.10.1",
    "antd": "^4.15.2",
    "antd-img-crop": "^3.14.1",
    "canvas-confetti": "^1.4.0",
    "file-saver": "^2.0.5",
    "form-render": "^1.3.1",
    "html2canvas": "^1.0.0-rc.7",
    "jszip": "^3.6.0",
    "moveable": "^0.24.6",
    "pattern.css": "^1.0.0",
    "react-draggable": "^4.4.3",
    "react-rnd": "^10.2.4",
    "umi": "^3.4.11",
    "umi-plugin-tailwindcss": "^3.0.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.11",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
